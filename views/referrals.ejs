<h1>Referrals for <%= readerName %></h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% referrals.forEach(function(referral) { %>
            <tr>
                <td><%= referral.name %></td>
                <td><%= referral.email %></td>
            </tr>
        <% }) %>
    </tbody>
</table>
